{
  "name": "@antsask/portfolio",
  "version": "0.1.0",
  "repository": "git@github.com:AnTSaSk/Portfolio.git",
  "author": "Alexis Besson <alexis.bessonweb@gmail.com>",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "lint": "yarn workspaces foreach run lint",
    "lint:styleguide": "yarn workspace @antsask/styleguide run lint",
    "lint:website": "yarn workspace @antsask/website run lint",
    "lint:style": "yarn workspaces foreach run lint:style",
    "lint:style:styleguide": "yarn workspace @antsask/styleguide run lint:style",
    "lint:style:website": "yarn workspace @antsask/website run lint:style",
    "lint:markup": "yarn workspaces foreach run lint:markup",
    "lint:markup:styleguide": "yarn workspace @antsask/styleguide run lint:markup",
    "lint:markup:website": "yarn workspace @antsask/website run lint:markup",
    "dev:styleguide": "yarn workspace @antsask/styleguide run dev",
    "dev:website": "yarn workspace @antsask/website run dev",
    "build:styleguide": "yarn workspace @antsask/styleguide run build",
    "build:website": "yarn workspace @antsask/website run build",
    "build": "yarn workspaces foreach run build"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "@vitejs/plugin-vue": "4.2.3",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.13.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.8",
    "sass": "1.62.1",
    "stylelint": "15.6.2",
    "stylelint-order": "6.0.3",
    "typescript": "5.0.4",
    "vue-eslint-parser": "9.3.0"
  },
  "packageManager": "yarn@3.4.1"
}
