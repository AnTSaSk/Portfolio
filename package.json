{
  "name": "@antsask/portfolio",
  "version": "0.1.0",
  "repository": "git@github.com:AnTSaSk/Portfolio.git",
  "author": "Alexis Besson <alexis.bessonweb@gmail.com>",
  "private": true,
  "scripts": {
    "postinstall": "husky",
    "lint": "pnpm -r run lint",
    "lint:styleguide": "pnpm --filter @antsask/styleguide lint",
    "lint:website": "pnpm --filter @antsask/website lint",
    "lint:style": "pnpm -r run lint:style",
    "lint:style:styleguide": "pnpm --filter @antsask/styleguide lint:style",
    "lint:style:website": "pnpm --filter @antsask/website lint:style",
    "lint:markup": "pnpm -r run lint:markup",
    "lint:markup:styleguide": "pnpm --filter @antsask/styleguide lint:markup",
    "lint:markup:website": "pnpm --filter @antsask/website lint:markup",
    "dev:styleguide": "pnpm --filter @antsask/styleguide dev",
    "dev:website": "pnpm --filter @antsask/website dev",
    "build:styleguide": "pnpm --filter @antsask/styleguide build",
    "build:website": "pnpm --filter @antsask/website build",
    "build": "pnpm -r run build",
    "generate:components": "npx tsx bin/components.ts",
    "test": "pnpm -r run test",
    "test:styleguide": "pnpm --filter @antsask/styleguide test",
    "test:website": "pnpm --filter @antsask/website test",
    "test:coverage": "pnpm -r run test:coverage"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "@vitejs/plugin-vue": "6.0.4",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-vue": "10.7.0",
    "eslint-plugin-vuejs-accessibility": "2.4.2",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss-scss": "4.0.9",
    "prettier": "3.8.1",
    "sass": "1.97.3",
    "stylelint": "17.3.0",
    "stylelint-order": "7.0.1",
    "typescript": "5.9.3",
    "vue-eslint-parser": "10.3.0"
  },
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.9.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
