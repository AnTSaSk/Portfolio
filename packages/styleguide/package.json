{
  "name": "@antsask/styleguide",
  "version": "0.1.0",
  "repository": "git@github.com:AnTSaSk/Portfolio.git",
  "author": "Alexis Besson <alexis.bessonweb@gmail.com>",
  "private": true,
  "module": "src/index.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "lint": "npx eslint --ext .ts,.vue --ignore-path ./../../.gitignore .",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint:markup": "vue-tsc --noEmit",
    "build": "yarn lint:markup && storybook build -o ../../dist/styleguide"
  },
  "lint-staged": {
    "*.{ts,vue}": "yarn run lint",
    "*.{css,scss}": "yarn run lint:style"
  },
  "dependencies": {
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@storybook/addon-actions": "7.0.2",
    "@storybook/addon-docs": "7.0.2",
    "@storybook/addon-essentials": "7.0.2",
    "@storybook/addon-interactions": "7.0.2",
    "@storybook/addon-links": "7.0.2",
    "@storybook/testing-library": "0.1.0",
    "@storybook/vue3": "7.0.2",
    "@storybook/vue3-vite": "7.0.2",
    "babel-loader": "9.1.2",
    "eslint-plugin-storybook": "0.6.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.2",
    "storybook-addon-sass-postcss": "0.1.3",
    "vite": "4.2.1",
    "vue-docgen-api": "4.64.1",
    "vue-tsc": "1.2.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "@vitejs/plugin-vue": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-vue": "*",
    "sass": "*",
    "stylelint": "*",
    "stylelint-order": "*",
    "typescript": "*",
    "vue-eslint-parser": "*"
  }
}
