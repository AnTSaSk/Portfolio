{
  "name": "@antsask/styleguide",
  "version": "0.1.0",
  "repository": "git@github.com:AnTSaSk/Portfolio.git",
  "author": "Alexis Besson <alexis.bessonweb@gmail.com>",
  "private": true,
  "type": "module",
  "module": "src/index.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "lint": "npx eslint .",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "lint:markup": "vue-tsc --noEmit",
    "build": "pnpm lint:markup && storybook build -o ../../dist/styleguide",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "10.2.8",
    "@storybook/addon-docs": "10.2.8",
    "@storybook/addon-links": "10.2.8",
    "@storybook/vue3": "10.2.8",
    "@storybook/vue3-vite": "10.2.8",
    "@vitejs/plugin-vue": "6.0.4",
    "@vitest/coverage-v8": "4.0.18",
    "@vue/test-utils": "2.4.6",
    "eslint-plugin-storybook": "10.2.8",
    "jsdom": "28.1.0",
    "storybook": "10.2.8",
    "vite": "7.3.1",
    "vitest": "4.0.18",
    "vue-docgen-api": "4.79.2",
    "vue-tsc": "3.2.4"
  },
  "peerDependencies": {
    "vue": "3.5.28"
  }
}
